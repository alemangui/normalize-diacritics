{
  "extends": [
    "eslint:recommended",
    "google",
    "xo-space",
    "xo-space/esnext"
  ],
  "plugins": [
    "security"
  ],
  "rules": {
    "no-console": [
      "error",
      { "allow": [ "log", "error", "warn" ] }
    ],
    "object-curly-spacing": [
      "error",
      "always"
    ],
    "max-len": "off",
    "valid-jsdoc": [
      "error",
      { "prefer": { "return": "returns" } }
    ]
  }
}
