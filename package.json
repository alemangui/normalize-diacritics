    "babel-preset-env": "^1.6.1",
    "jest": "^23.6.0",
    "rollup": "^1.1.0",
    "rollup-plugin-filesize": "^6.0.0",
    "rollup-plugin-terser": "^4.0.2",
    "rollup-plugin-tslint": "^0.2.2",
    "rollup-plugin-typescript": "^1.0.0",
