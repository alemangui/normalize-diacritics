{
  "name": "normalize-diacritics",
  "version": "0.4.0",
  "description": "Normalize diacritics from strings",
  "keywords": [
    "diacritic",
    "diacritics",
    "motss",
    "normalize",
    "normalize-diacritics",
    "npm"
  ],
  "homepage": "https://github.com/motss/normalize-diacritics",
  "license": "MIT",
  "author": "motss <wes.ngrongsen@gmail.com>",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "git@github.com:motss/normalize-diacritics.git"
  },
  "scripts": {
    "build": "npm run ts && cross-env NODE_ENV=production gulp build && rm -rf gulpfile.js",
    "codecov": "nyc report --reporter=lcov > coverage.lcov && codecov",
    "coveralls": "nyc report --reporter=text-lcov | coveralls",
    "gulp": "gulp build",
    "lint": "tslint --project tsconfig.json --config tslint.json",
    "lint:fix": "tslint -p . -c tslint.json --fix",
    "nsp": "nsp check",
    "nyc": "nyc report --reporter=lcov",
    "prepare": "npm run lint && npm run nsp && npm run build",
    "test": "nyc ava && npm run nyc",
    "ts": "tsc gulpfile.ts"
  },
  "devDependencies": {
    "@std/esm": "^0.17.2",
    "@types/node": "^8.0.53",
    "ava": "^0.24.0",
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-function-bind": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-minify": "^0.2.0",
    "codecov": "^3.0.0",
    "coveralls": "^3.0.0",
    "cross-env": "^5.1.1",
    "del": "^3.0.0",
    "gulp": "^3.9.1",
    "gulp-babel": "^7.0.0",
    "gulp-sequence": "^0.4.6",
    "gulp-typescript": "^3.2.3",
    "nsp": "^3.1.0",
    "nyc": "^11.3.0",
    "tslint": "^5.8.0",
    "tslint-config-airbnb": "^5.4.2",
    "tslint-config-prettier": "^1.6.0",
    "typescript": "^2.6.2"
  },
  "engines": {
    "node": ">= 8.9.0",
    "npm": ">= 5.5.1"
  },
  "nyc": {
    "exclude": [
      "{,**}/!(index.js)",
      "test",
      "test{,-*}.js",
      "**/*.test.js",
      "**/__tests__/**",
      "**/node_modules/**"
    ]
  },
  "dependencies": {},
  "@std/esm": "cjs",
  "ava": {
    "files": [
      "dist/test/**/*.js"
    ],
    "require": [
      "@std/esm"
    ]
  }
}
